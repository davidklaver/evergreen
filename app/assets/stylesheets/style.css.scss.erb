:root {
  --lightgreen: #8ec63f;
  --darkgreen: #01663c;
}

html, body {
  margin: 0;
  padding: 0;
  overflow-x: hidden;
}

h1,h2,h3,h4,h5,h6 {
  text-align: center;
}

.navbar-brand {
  padding-top: 5px;
}

.navLogo {
  width: 50px;
  visibility: hidden;
}

.visible {
  visibility: visible;
}

@media(max-width: 767px) {
  .navLogo {
    width: 50px;
  }
}

#menu{
  height: 75px;
}

#menu ul {
  margin-top: -45px;
}

#footer h3 {
  font-family: 'Runaway';
  font-size: 36px;
  color: white;
}

.container{
  width: 100vw;
  margin-top: 100px;
}

.navbar-nav {
  padding: 2px 50px;
  /*border-bottom: 1px solid var(--lightgreen);*/
}

@media(min-width: 768px) {
  .navbar-nav {
    width: 100vw;
    display: flex;
    /*margin-left: -50px;*/
    justify-content: space-around;
  }

  .page-scroll {
    font-family: font86429;
    /*font-weight: bold;*/
    font-size: 24px;
    color: var(--darkgreen) !important;
    /*text-transform: uppercase;*/
    /*color: white !important;*/
  }

}

.chesed247Link::before {
      content: "New!";
      font-size: 20px;
      color: #efd;
      position: absolute;
      top: 2px;
      left: -5px;
      transform: rotate(-20deg);
    }

@media(max-width: 767px) {
  .navbar-nav {
    padding: 0 15px;
    a.page-scroll {
      text-align: center;
      color: white !important;
      font-size: 18px;
    }
  }
}

.navbar {
  background-color: transparent;
  /*background-color: rgba(255,255,255,0.5) !important;*/
  /*background-color: rgba(10,90,40,.8);*/
  border: none;
  padding: 0 !important;
  padding: 5px 0 !important;
}

@media(max-width: 767px) {
  .navbar ul {
    background-color: rgba(10,90,40,.8);
    li {
      padding: 5px;
      &:hover {
        background-color: var(--lightgreen) !important;
      }
    }
  }
}


