.gradientOverlay {
  width: 100vw;
  height: 600px;
  position: absolute;
  background: rgb(2,0,36);
  background: linear-gradient(90deg, rgba(2,0,36,0) 0%, rgba(255,255,255,0.5452556022408963) 33%, rgba(255,255,255,0.8029586834733894) 50%, rgba(255,255,255,1) 54%, rgba(255,255,255,1) 100%);
}

@media(max-width: 768px) {

  .gradientOverlay {
    height: 100%;
    background: rgba(255,255,255,.7);
  }
}

