

html, body {
  margin: 0;
  padding: 0;
  overflow-x: hidden;
}

h1,h2,h3,h4,h5,h6 {
  text-align: center;
}

.navbar-brand {
  padding-top: 5px;
  padding-left: 7vw;
}

.navLogo {
  width: 200px;
  /*visibility: hidden;*/
}

.visible {
  visibility: visible;
}

@media(max-width: 1199px) {
  .navLogo {
    width: 150px;
  }
}

.navbar {
  height: 75px;
}

#menu ul {
  margin-top: -45px;
}

#footer h3 {
  font-family: 'Runaway';
  font-size: 36px;
  color: white;
}

.container{
  width: 100vw;
  margin-top: 100px;
}

.navbar-nav {
  padding: 2px 10px;
  /*border-bottom: 1px solid var(--lightgreen);*/
}

.nav a:hover {
  color: var(--lightgreen) !important;
}

@media(min-width: 1200px) {
  .navbar-nav {
    width: 75vw;
    margin-left: 20vw;
    display: flex;
    /*margin-left: -50px;*/
    justify-content: space-around;
  }

  .page-scroll {
    font-family: 'markdown';
    letter-spacing: 3px;
    font-size: 18px;
    /*color: var(--darkgreen) !important;*/
    /*text-transform: uppercase;*/
    color: white !important;
  }
}

.chesed247Link::before {
      content: "New!";
      /*background: url("<%= asset_path('saleBubble.png') %>") no-repeat center center;
      background-size: contain;*/
      padding: 15px;
      font-size: 14px;
      color: var(--lightgreen);
      position: absolute;
      top: -10px;
      left: -15px;
      transform: rotate(-20deg);
    }

@media(max-width: 1199px) {
  .navbar-nav {
    padding: 0 15px;
    a.page-scroll {
      text-align: center;
      color: white !important;
      font-size: 18px;
    }
  }
}

.navbar {
  
  /*background-color: rgba(255,255,255,0.5) !important;*/
  /*background-color: rgba(10,90,40,.8);*/
  background-color: var(--darkgreen);
  border: none;
  
}

@media(max-width: 1199px) {
  .navbar ul {
    /*background-color: rgba(10,90,40,.8);*/
    background-color: var(--lightgreen);
    li {
      padding: 5px;
      &:hover {
        background-color: var(--darkgreen) !important;
      }
    }
  }
}


