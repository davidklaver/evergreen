.storeLinksDiv {
  background: url("<%= asset_path('home_bg8.jpg') %>") center center fixed;
  background-size: cover;
  padding: 50px 5vw;
  width: 100vw;
  /*margin-left: 5vw;*/
}
.storeLinkDiv img {
  width: 20vw;
  margin: 10px;
  padding: 5px;
  border: 2px solid #eee;
  border-radius: 25px;
  box-shadow: 10px 10px 10px black, 10px 10px 10px var(--darkgreen);
  position: relative;
  z-index: 1000;
  transition: .25s;
}

.storeLinksDivOverlay {
  width: 100vw;
  height: 100%;
  
}

@media(max-width: 960px) {
  .storeLinkDiv img {
    width: 40vw;
    /*margin-left: 0;*/
  }
}

@media(min-width: 960px) {
  .storeLinkDiv img:hover {
    box-shadow: 20px 20px 20px black, 20px 20px 20px var(--darkgreen);
    width: calc(20vw + 10px);
    margin: 5px;
  }
}


@media(max-width: 479px) {
  .storeLinkDiv img {
    width: 90vw;
    margin-left: 0;
  }
}

